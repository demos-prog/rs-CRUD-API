{
  "name": "rs-crud-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev src/server.ts",
    "start:prod": "tsc && node dist/server.js",
    "jest":"jest --silent",
    "test": "start-server-and-test start:dev http://localhost:5000 jest",
    "build": "tsc"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(test).ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.5",
    "rs-crud-api": "file:",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
